Algoritmo sin_titulo
	
	Definir m2 Como Entero;
	Definir pm, pT Como Real;
	
	
	Escribir "Ingrese el precio por m2";
	leer pm;
	
	Escribir "Ingrese la cantidad de m2 a pintar";
	Leer m2;
	
	pT = pm * m2;
	
	Escribir "El total a pagar por la pintada es de: ", pT, " dolares";
	
FinAlgoritmo
